if (<%= @renderPage %>) {
  const page = document.querySelector('.page');

  page.innerHTML = `
    <%= escape_javascript( render('nav')) %>
    <%= escape_javascript( render('location')) %>
    <%= escape_javascript( render('event-list')) %>
  `;

  //Initialize slider
  sliderLogic.sliderInit();

  //Re-initialize event listeners
  eventsLogic.eventsInit();

  //Filter if had previous terms
  eventsLogic.filter();

}